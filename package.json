  "author": "Dima Viron vivo@gmail.com",
  "scripts": {
    "react:server:dev": "webpack serve --no-open --env port=3000",
    "electron:server:dev": "cross-env NODE_ENV=development electronmon -r ts-node/register/transpile-only .",
    "electron:server:prod": "NODE_ENV=production electron .",
    "build:package:electron": "rm -rf src/electron-app/build && cp -r build/ src/electron-app/build && electron-builder -l -w",
    "static:build:dev:browser": "webpack --env mode=development",
    "static:build:prod:browser": "webpack --env mode=production baseRoute=/jira-time-tracker buildEnv=browser",
    "static:build:prod:electron": "webpack --env mode=production baseRoute=/ buildEnv=electron",
    "lint:ts": "../node_modules/.bin/eslint \\\"**/*.{ts,tsx,js,jsx}\\\"",
    "lint:ts:fix": "../node_modules/.bin/eslint \\\"**/*.{ts,tsx,js,jsx}\\\" --fix",
    "lint:css": "yarn ../node_modules/.bin/stylelint \\\"src/**/*.css\\\"",
    "lint:css:fix": "yarn ../node_modules/.bin/stylelint \\\"src/**/*.css\\\" --fix",
    "test:unit": "jest"
  },
  "build": {
    "appId": "com.example.jiratimetracker",
    "extraResources": [
      ".env.production"
    ],
    "linux": {
      "maintainer": "Dima Viron vivo@gmail.com",
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "build/icons/"
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ],
      "icon": "build/icons/",
      "publisherName": "Dima Viron vivo@gmail.com"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
